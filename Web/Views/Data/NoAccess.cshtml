@{
    ViewBag.Title = "Přístup zamítnut";
}


@section breadcrumb
{
    <ol class="breadcrumb">
        <li><a href="/">Hlídač Státu</a></li>
        <li><a href="/data">Další databáze</a></li>
        <li class="active">@ViewBag.Title</li>
    </ol>
    <div class="active float-end"><a href="/data/manage" class="btn btn-hs btn-primary btn-sm" style="color:whitesmoke;text-decoration:none">Snadné přidání nového datasetu</a></div>
}

<div class="row">

    <div class="col-xs-12">
        <h3>Problém</h3>
        <p>
            K tomuto datasetu nemáte přístup. <a href="/data/Index/@ViewBag.DatasetId">Kontaktujte autora</a> a požádejte ho o spolupráci. V případě jeho nedostupnosti <a href="/texty/kontakt">napište nám</a>.
        </p>
        <a href="/data" class="btn btn-hs btn-primary">Vytvořit s JSON Schema</a>
    </div>

</div>
