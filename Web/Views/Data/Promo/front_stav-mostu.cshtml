@using HlidacStatu.Datasets
@model object

@{
    Layout = null;

    DataSet ds =
        DataSet.CachedDatasets.Get((string)Model);
    var reg = await ds.RegistrationAsync();

}
<div class="row">
    <div class="col-xs-12">
        <h3>@reg.name</h3>
        <p>@reg.description</p>

        <iframe src="https://www.google.com/maps/d/u/0/embed?mid=1q2tP7lpTxs1AxzG1NO6a6-HYxl0jmqQq" width="800" height="600"></iframe>

    </div>
</div>
