@model string
@{

    ViewBag.Title = "Hlídač Dotací: Hledání";
    ViewBag.SubTitle = Model;

    ViewBag.ShowBookmarkUrl = true;
}


@section breadcrumb
{
    <ol class="breadcrumb">
        <li><a href="/">Hlídač Státu</a></li>
        <li><a href="@Url.Action("Index","Dotace")">Hlídač Dotací</a></li>
        <li>@ViewBag.SubTitle</li>
    </ol>
}

<div>
    @Model
</div>

